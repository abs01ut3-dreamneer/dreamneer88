		<!-- <video src="video/intro.mp4" class="testimonials-bg" style="height: 400px; object-fit: cover; opacity: 0.3;" autoplay muted loop playsinline ></video> -->
		<div class="swiper init-swiper" style="padding-top: 25px;">
			<script type="application/json" class="swiper-config">
            {
              "loop": true,
              "speed": 600,
              "autoplay": {
