<!-- //////////////////////////공지 Section//////////////////////////// -->
<section id="notice" class="services-2 section light-background">
    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>공지</h2>
        <a href="/notice/list" class="notice-main-link">
            전체 공지사항 보기
        </a>
        <p>CHECK OUR SERVICES</p>
    </div>

    <div class="container">
        <!-- 메인 공지사항 카드 -->
        <div id="noticeCard" class="notice-main-container">
            <div class="notice-main-header">
                <h6 class="notice-main-title">공지사항</h6>
                <div class="notice-main-subtitle">최근 공지</div>
            </div>

            <div class="notice-main-body">
                <div class="table-responsive">
                    <table class="notice-main-table">
                        <thead class="notice-main-thead">
                            <tr class="notice-main-header-row">
                                <th class="notice-main-col-number">순번</th>
                                <th class="notice-main-col-title">제목</th>
                                <th class="notice-main-col-date">작성일</th>
                            </tr>
                        </thead>
                        <tbody>
                            <c:forEach var="noticeVO" items="${articlePage.content}">
                                <!-- ✅ data-notice-sn으로 변경! -->
                                <tr class="notice-main-row" data-notice-sn="${noticeVO.noticeSn}">
                                    <td class="notice-main-cell-number">${noticeVO.noticeSn}</td>
                                    <td class="notice-main-cell-title">
                                        <a href="#" class="notice-main-link-text" data-notice-sn="${noticeVO.noticeSn}">
                                            <span class="notice-main-title-text">${noticeVO.noticeSj}</span>
                                        </a>
                                    </td>
                                    <td class="notice-main-cell-date">
                                        <fmt:formatDate value="${noticeVO.noticeWritngDt}" pattern="yyyy-MM-dd" />
                                    </td>
                                </tr>
                            </c:forEach>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
               
               
