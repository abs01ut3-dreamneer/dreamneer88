<!-- /// 캘린더 모달 시작 /// -->
<div class="modal fade" id="calendarModal" tabindex="-1" aria-labelledby="detailModalLabel" aria-hidden="true">
   <div class="modal-dialog modal-lg">
      <div class="modal-content" style="border: 0; border-radius: 14px; box-shadow: 0 6px 24px rgba(0, 0, 0, .12); overflow: hidden;">
         
         <!-- 헤더 -->
         <div class="modal-header" style="background: rgba(100, 140, 164, .85); color: #fff; padding: .6rem .9rem; border: none;">
            <h5 class="modal-title" id="detailModalLabel" style="font-size: 1rem; margin: 0;">상세 보기</h5>
            <button type="button" class="btn-close btn-close-white" data-bs-dismiss="modal" aria-label="닫기"></button>
         </div>

         <!-- 바디 -->
         <div class="modal-body" style="padding: .9rem;">
            <form id="detailscheduleForm">
               <!-- 숨겨진 담당자 정보 -->
               <div class="form-group row" style="display: none;">
                  <label class="col-sm-2 col-form-label" style="color: #6b7b86; font-size: .75rem; font-weight: 600;">담당자</label>
                  <div class="col-sm-10">
                     <input type="hidden" id="calendarempId" name="calendarempId" class="form-control form-control-sm" />
                  </div>
               </div>
               
               <!-- 제목 -->
               <div class="form-group row">
                  <label class="col-sm-2 col-form-label" style="color: #6b7b86; font-size: .75rem; font-weight: 600; padding-left:40px; margin-bottom:10px;">제목</label>
                  <div class="col-sm-10">
                     <input type="text" id="calendarSj" name="calendarSj" class="form-control form-control-sm" style="font-size: .85rem;" readonly />
                  </div>
               </div>
               
               <!-- 구분 -->
               <div class="form-group row" style="display: none;">
                  <label class="col-sm-2 col-form-label" style="color: #6b7b86; font-size: .75rem; font-weight: 600; padding-left:40px;">구분</label>
                  <div class="col-sm-10">
                     <select id="calendarIem" name="calendarIem" class="form-control form-control-sm" style="font-size: .85rem;">
                        <option value="">선택</option>
                        <option value="시설">시설</option>
                        <option value="커뮤니티">커뮤니티</option>
                        <option value="개인일정">개인일정</option>
                     </select>
                  </div>
               </div>
               
               <!-- 시작일시 및 종료일시 -->
               <div class="form-group row">
                  <label class="col-sm-2 col-form-label" style="color: #6b7b86; font-size: .75rem; font-weight: 600; padding-left:40px; margin-bottom:10px;">시작일시</label>
                  <div class="col-sm-4">
                     <input type="datetime-local" id="calendarBeginDt" name="calendarBeginDt" class="form-control form-control-sm" style="font-size: .85rem;" readonly />
                  </div>
                  <label class="col-sm-2 col-form-label text-right" style="color: #6b7b86; font-size: .75rem; font-weight: 600; padding-left:60px;">종료일시</label>
                  <div class="col-sm-4">
                     <input type="datetime-local" id="calendarEndDt" name="calendarEndDt" class="form-control form-control-sm" style="font-size: .85rem;" readonly />
                  </div>
               </div>
               
               <!-- 내용 -->
               <div class="form-group row">
                  <label class="col-sm-2 col-form-label" style="color: #6b7b86; font-size: .75rem; font-weight: 600; padding-left:40px; margin-bottom:10px;">내용</label>
                  <div class="col-sm-10">
                     <textarea id="calendarCn" name="calendarCn" class="form-control form-control-sm" rows="3" style="resize: none; font-size: .85rem;" readonly></textarea>
                  </div>
               </div>

               <!-- 장소 -->
               <div class="form-group row">
                  <label class="col-sm-2 col-form-label" style="color: #6b7b86; font-size: .75rem; font-weight: 600; padding-left:40px; margin-top:8px;">장소</label>
                  <div class="col-sm-10">
                     <input type="text" id="calendarPlace" name="calendarPlace" class="form-control form-control-sm" style="font-size: .85rem; margin-top:10px;" readonly />
                  </div>
               </div>
            </form>
         </div>

         <!-- 푸터 -->
         <div class="modal-footer" style="padding: .6rem .9rem; background: #fff; border: none;"></div>
      </div>
   </div>
</div>

<!--캘린더 모달 끝-->
