<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.mapper.UntpcMapper">
    <select id="getActiveRates" parameterType="untpcVO" resultType="untpcVO">
        SELECT
            UNTPC_ID,
            IEM_NM,
            RQEST_UNTPC,
            END_DE
        FROM
            UNTPC
        WHERE
            END_DE IS NULL
    </select>
</mapper>